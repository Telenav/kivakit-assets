@startuml

    !include lexakai.theme
    
    title "com.telenav.kivakit.kernel.language.reflection.access.field"
    
    interface Getter
    interface Setter
    
    Getter <|-- FieldGetter
    class FieldGetter
    {
        --(constructors)--
        +FieldGetter(java.lang.reflect.Field)
        --
        +java.lang.reflect.Field field()
    }
    
    Setter <|-- FieldSetter
    class FieldSetter
    {
        --(constructors)--
        +FieldSetter(java.lang.reflect.Field)
    }
    
@enduml

