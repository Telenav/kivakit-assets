@startuml

    !include lexakai.theme
    
    title "diagram-factory"
    
    interface Source
    Source <|-- Factory
    interface Factory<Value>
    {
        --(factory)--
        +Value newInstance()
    }
    
    interface IntMapFactory<Value>
    {
        --(factory)--
        +Value newInstance(int parameter)
    }
    
    interface LongMapFactory<Value>
    {
        --(factory)--
        +Value newInstance(long parameter)
    }
    
    interface MapFactory<Parameter, Value>
    {
        --(factory)--
        +Value newInstance(Parameter)
    }
    
@enduml

